{
  "eventbus": {
    "description": "This is the event bus for our imaginary company. You can customise the description and add metadata to all your events!"
  },
  "deadhappy.claims": {
    "events": {
      "ClaimCreated": {
        "description": "Event raised when a claim is created for some application products.",
        "properties": {
          "applicationProductIds": "List of application products to claim for",
          "status": "Is the claim accepted/reject?"
        }
      }
    }
  },
  "deadhappy.applications": {
    "metadata": {
      "title": "All events raised within the application domain."
    },
    "events": {
      "FlexApplicationConfirmed": {
        "description": "Event is raised when the user flexs their application using the mobile application.",
        "properties": {
          "newCoverAmount": "The new amount the user will be covered for",
          "previousCoverAmount": "The previous amount the user was covered for"
        },
        "example": {
          "Time": "2020-07-14T00:00:00Z",
          "Source": "deadhappy.applications",
          "Resources": ["1"],
          "DetailType": "FlexApplicationConfirmed",
          "Detail": {
            "data": {
              "brand": {
                "code": "00001"
              },
              "user": {
                "email": "test@test.com",
                "id": 1
              },
              "newCoverAmount": 1000,
              "previousCoverAmount": 1000
            },
            "metadata": {
              "correlationId": "5e2ad09c-c502-11eb-8529-0242ac130003",
              "service": "myapp-some-service"
            }
          },
          "EventBusName": "myapp-event-bus-dev"
        }
      }
    }
  },
  "deadhappy.deathwishes": {
